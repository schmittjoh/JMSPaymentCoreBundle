{
    "name": "jms/payment-core-bundle",
    "type": "symfony-bundle",
    "description": "This bundle is providing the foundation for various payment plugins.",
    "keywords": ["payment"],
    "homepage": "http://jmsyst.com/bundles/JMSPaymentCoreBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TicketSwap/JMSPaymentPaypalBundle"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0|~3.1",
        "ext-mcrypt": "*"
    },
    "require-dev": {
        "symfony/class-loader": "^3.1",
        "symfony/twig-bundle": "^3.1",
        "symfony/finder": "^3.1",
        "symfony/browser-kit": "^3.1",
        "symfony/css-selector": "^3.1",
        "symfony/form": "^3.1",
        "symfony/validator": "^3.1",
        "symfony/process": "^3.1",
        "symfony/yaml": "^3.1",
        "sensio/framework-extra-bundle": "^3.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "ticketswap/payment-paypal-bundle": "dev-feature/fix-deprecations",
        "jms/aop-bundle": "*",
        "jms/di-extra-bundle": "*",
        "phpunit/phpunit": "^5.4",
        "symfony/phpunit-bridge": "^3.1"
    },
    "autoload": {
        "psr-0": { "JMS\\Payment\\CoreBundle": "" }
    },
    "target-dir": "JMS/Payment/CoreBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
