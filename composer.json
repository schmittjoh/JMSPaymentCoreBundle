{
    "name": "gsoi/jms-payment-core-bundle",
    "type": "symfony-bundle",
    "description": "A unified API for processing payments with Symfony",
    "keywords": ["payment"],
    "homepage": "http://jmspaymentcorebundle.readthedocs.io",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        },
        {
            "name": "Paulo Rodrigues Pinto",
            "email": "regularjack@gmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/schmittjoh/JMSPaymentCoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "gsoi/test-pack": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "JMS\\Payment\\CoreBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JMS\\Payment\\CoreBundle\\Tests\\": "tests/"
        }
    },

    "config": {
        "github-oauth": {
            "github.com": "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"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "target-dir": "JMS/Payment/CoreBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs", "url": "git@github.com:GSOinteractive/php-test-pack.git"
        }
    ]
}
